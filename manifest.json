{
    "manifest_version": 2,
    "name": "HoursWatched",
    "version": "1.0.2",
    "icons": {
        "48": "imagens/icon48.png"  
    },
    "browser_action": {
        "default_icon": "imagens/icon16.png"  
      },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "css": [
                "style.css"
            ],
            "js": [
                "content.js"
            ]
        }
    ]

}